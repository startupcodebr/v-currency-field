{
  "name": "v-currency-field",
  "version": "3.1.1",
  "description": "Currency component for vuetify v-text-field",
  "author": "Philipe Augusto <phiny1@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/phiny1/v-currency-field.git"
  },
  "keywords": [
    "vue",
    "vuetify",
    "input",
    "mask",
    "currency",
    "money"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "demo": "vue-cli-service serve ./demo/main.js",
    "docs": "npm run docs:serve",
    "docs:build": "vue-cli-service docs --mode build",
    "docs:serve": "vue-cli-service docs --mode serve",
    "prepublish": "vue-cli-service lint && NODE_ENV=production vue-cli-service docs --mode build && vue-cli-service build",
    "start": "vue-cli-service serve"
  },
  "main": "dist/v-currency-field.common.js",
  "module": "dist/v-currency-field.esm.js",
  "unpkg": "dist/v-currency-field.umd.min.js",
  "files": [
    "dist/",
    "dist/v-currency-field.common.js",
    "dist/v-currency-field.umd.min.js",
    "dist/v-currency-field.umd.js",
    "dist/v-currency-field.esm.js",
    "src",
    "nuxt",
    "nuxt-treeshaking"
  ],
  "dependencies": {},
  "devDependencies": {
    "@phiny1/vue-cli-plugin-p11n": "^0.4.1",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "sass": "^1.23.3",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-vuetify": "^2.0.2",
    "vue-currency-input": "^1.22.6",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.3.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jsdelivr": "dist/v-currency-field.umd.min.js",
  "sideeffects": false,
  "size-limit": [
    {
      "path": "dist/v-currency-field.js",
      "limit": "4.0 KB"
    }
  ],
  "peerDependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.10",
    "vuetify": "^2.2.28"
  }
}
